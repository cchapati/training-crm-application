{% block orocrm_partner_git_hub_account_widget %}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.name) }}
    {% if form.additional is defined and form.additional.children|length > 0 %}
        {% for value in form.additional %}
            {{ form_row(value) }}
        {% endfor %}
    {% endif %}
{% endblock %}
